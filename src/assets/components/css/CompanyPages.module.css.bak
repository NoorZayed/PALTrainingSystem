}

.detailLabel {
  width: 150px;
  font-weight: 600;
  color: #555;
}

.detailValue {
  flex: 1;
}

.supervisorInfo {
  display: flex;
  flex-direction: column;
}

.supervisorEmail {
  color: #666;
